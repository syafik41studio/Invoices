<div class="subcolumns">
  <% @followings.each do |follow|  %>
    <div class="c20l" style="height: 100px;">
      <% user = follow.following_user %>
      <div style="text-align: center">
        <%= link_to photo_profile(user.profile), user_path(user)  %>
        <h6><%= link_to user.full_name, user_path(user) %></h6>
      </div>
    </div>
  <% end %>
</div>
